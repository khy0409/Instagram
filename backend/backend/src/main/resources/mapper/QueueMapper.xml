<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="vue.example.backend.Mapper.Queuemapper">

    <!-- UserId로 사용자 조회 -->
    <select id="UserId" resultType="hashmap">
        SELECT * 
        FROM instagram_user 
        WHERE id = #{UserId}
    </select>

</mapper>
